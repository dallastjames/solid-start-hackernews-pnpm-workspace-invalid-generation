#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/dallasjames/Projects/Personal/solid-hackernews/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/dallasjames/Projects/Personal/solid-hackernews/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/solid-start@0.2.3_ezjg2l4vevr6tsyo6l36dhwiw4/node_modules/solid-start/bin.cjs" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/solid-start@0.2.3_ezjg2l4vevr6tsyo6l36dhwiw4/node_modules/solid-start/bin.cjs" "$@"
fi
